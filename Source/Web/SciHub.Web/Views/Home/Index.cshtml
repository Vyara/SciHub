@{
    ViewBag.Title = "Home Page";
}
<div class="well row">
    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
            <li data-target="#carousel-example-generic" data-slide-to="1"></li>
            <li data-target="#carousel-example-generic" data-slide-to="2"></li>
            <li data-target="#carousel-example-generic" data-slide-to="3"></li>
            <li data-target="#carousel-example-generic" data-slide-to="4"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="http://orig11.deviantart.net/4a05/f/2012/301/a/2/coffeepainting__sci_fi_city_by_macrebisz-d5j78y1.jpg" alt="...">
                <div class="carousel-caption">
                    <h2>Welcome to SciHub!</h2>
                </div>
            </div>
            <div class="item">
                <img src="http://www.coolwallpapers.id/wp-content/uploads/2016/02/wide-sci-fi-hd-pics.jpg" alt="...">
                <div class="carousel-caption">
                    <h2>Browse our <a href="/Movie/Home">Movies</a></h2>
                </div>
            </div>
            <div class="item">
                <img src="https://wallpaperscraft.com/image/book_lies_sofa_library_68253_1920x1080.jpg" alt="...">
                <div class="carousel-caption">
                    <h2>Look through our <a href="/Book/Home">Books</a></h2>
                </div>
            </div>
            <div class="item">
                <img src="http://eskipaper.com/images/sci-fi-wallpaper-14.jpg" alt="...">
                <div class="carousel-caption">
                    <h2>See the <a href="/TvShow/Home">TvShows</a></h2>
                </div>
            </div>
            <div class="item">
                <img src="http://www.samsdotpublishing.com/wp-content/uploads/2015/11/prompt4.jpg" alt="...">
                <div class="carousel-caption">
                    <h2>Enjoy the <a href="/ShortStory/Home">Short Stories</a></h2>
                </div>
            </div>

        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
        </a>
        <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
        </a>
    </div> <!-- Carousel -->
</div>
@section scripts{
    <script>
        $('.carousel').carousel({
            interval: 4000
        })
    </script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>

}